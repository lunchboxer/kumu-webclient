{
  "name": "kumu-webclient",
  "version": "0.2.0",
  "devDependencies": {
    "bulma": "^0.7.4",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-svelte3": "^0.4.5",
    "eslint-plugin-unicorn": "^8.0.1",
    "graphql": "^14.2.0",
    "graphql-cli": "^3.0.11",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "serve": "^10.1.2",
    "style-loader": "^0.23.1",
    "svelte": "beta",
    "svelte-loader": "2.13.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-modules": "^1.0.0"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:svelte",
    "build:css": "node-sass src/main.scss public/main.css",
    "build:svelte": "cross-env NODE_ENV=production webpack",
    "dev": "concurrently npm:watch-*",
    "watch-svelte": "webpack-dev-server --content-base public --host 0.0.0.0",
    "watch-css": "npm run build:css -- --watch",
    "serve": "serve -s public",
    "lint": "eslint src/**/*"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "cross-fetch": "^3.0.2",
    "flatpickr": "^4.5.7",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.1",
    "nanographql": "^2.0.0",
    "navaid": "^1.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "svelte-flatpickr": "^1.1.0"
  }
}